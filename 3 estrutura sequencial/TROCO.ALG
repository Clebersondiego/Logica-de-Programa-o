Algoritmo "troco"

Var
 quantidade: inteiro
troco, preco,  dinheiro: real


Inicio

  escreva(" Preço unitario do produto: ")
  leia(preco)
  escreva(" Quantidade comprada: ")
  leia(quantidade)
  escreva(" Dinheiro recebido: ")
  leia(Dinheiro)
  
  
    TROCO <- dinheiro - (preco * quantidade)
  

  escreval(" TROCO " , troco:4:2)


Fimalgoritmo